#!/system/bin/sh

cat << CTAG
{
    name:SELinux,
    elements:[
	{ SPane:{
		title:"SELinux Status",
		description:"Allows you to change your SELinux (Security-Enhanced Linux) Status from Enforcing (policy is enforced) to Permissive (prints warnings instead of enforcing). Default is Permissive.",
	}},
		{ SOptionList:{
			default:`getenforce`,
			action:"devtools selinux",
			values:[ Enforcing, Permissive
			],
		}},
    ]
}
CTAG
